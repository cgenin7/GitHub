@model IEnumerable<SalesTaxesModels.CategoryModel>

@{
    ViewBag.Title = "Index";
}

<h2>Categories</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Category)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PercentageOfBasicTaxes)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PercentageOfImportTaxes)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Category)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PercentageOfBasicTaxes)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PercentageOfImportTaxes)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.CategoryId }) |
            @Html.ActionLink("Details", "Details", new { id=item.CategoryId })
        </td>
    </tr>
}

</table>
